{
	"TSENSOR": {
        "inherits": ["FAMILY_STM32"],
        "core": "Cortex-M0+",
        "extra_labels_add": [
            "STM32L0",
            "STM32L072CZ",
            "STM32L072xZ",
            "STM32L072xx"
        ],
        "supported_form_factors": ["MIKROBUS"],
        "supported_toolchains": ["GCC_ARM"],
        "config": {
            "clock_source": {
                "help": "Mask value : USE_PLL_HSE_EXTC (need HW patch) | USE_PLL_HSE_XTAL (need HW patch) | USE_PLL_HSI",
                "value": "USE_PLL_HSI",
                "macro_name": "CLOCK_SOURCE"
            },
            "lpticker_lptim": {
                "help": "This target supports LPTIM. Set value 1 to use LPTIM for LPTICKER, or 0 to use RTC wakeup timer",
                "value": 1
            },
          	"se050-enapin": {
        		"help": "Name of the PIN connected to SE050 ENA Pin",
        		"value" : "PB_5"
        	},
          	"se050-sda": {
        		"help": "Name of the PIN connected to SE050 I2C SDA",
        		"value" : "PB_9"
        	},
          	"se050-scl": {
        		"help": "Name of the PIN connected to SE050 I2C SCL",
        		"value" : "PB_8"
        	},
          	"led": {
        		"help": "LED pin name",
        		"value" : "PH_1"
        	},
          	"uart-tx": {
        		"help": "UART TX pin name",
        		"value" : "PA_9"
        	},
          	"uart-rx": {
        		"help": "UART RX pin name",
        		"value" : "PA_10"
        	}
        },
        "macros_add": [
            "MBED_TICKLESS",
            "EXTRA_IDLE_STACK_REQUIRED"
        ],
        "overrides": { "lpticker_delay_ticks": 0 },
        "detect_code": ["0833"],
        "device_has_add": [
            "ANALOGOUT",
            "CRC",
            "SERIAL_ASYNCH",
            "TRNG",
            "FLASH",
            "MPU"
        ],
        "release_versions": ["2", "5"],
        "device_name": "STM32L072CZ"
    },
    "TSENSOR_DEV": {
        "inherits": ["TSENSOR"],
        "overrides": {
        	"target.se050-enapin": "PB_7",
        	"target.se050-sda": "PB_9",
        	"target.se050-scl": "PB_8",
        	"target.led": "PB_6",
 			"target.uart-tx": "PA_2",
			"target.uart-rx": "PA_3"
        }
    }
}
